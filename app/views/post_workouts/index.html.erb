<h1> Move and Groove Fitness Tracker</h1>
<h5><%= link_to "New workout", new_post_workout_path %></h2>
    
<% if @post_workouts %>
<% @post_workouts.each do |post_workout| %>
    <h5>Workout: <%= link_to post_workout.name, post_workout %></h2>
    <div>Category: <%= post_workout.category %>
    <div>Date: <%= post_workout.date %></div>
    <div>Duration: <%= post_workout.duration %></div>
<% end %>
<% end %>
